{
  "homepage": "https://shakmr.github.io/signature-templater",
  "name": "signature-templater",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "flow-watch": "fswatch -o ./ | xargs -n1 -I{} sh -c 'clear; printf \"\\033[3J\" && flow'",
    "flow_check": "flow check || exit 0",
    "postinstall": "flow-typed install react-router-dom",
    "start:dev": "NODE_ENV=development react-scripts start",
    "start:server": "node server/index.js",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bpk-mixins": "^19.0.10",
    "flow-bin": "^0.109.0",
    "flow-typed": "^2.6.2",
    "gh-pages": "^3.2.3",
    "jest-localstorage-mock": "^2.4.0",
    "nock": "^11.6.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.3"
  }
}
